# Module: Aliases

alias :q=exit

alias ls='ls -AlG'

# git stuff
alias gs="git status"
alias lol="git log --graph --decorate --oneline"
alias lola="git log --graph --decorate --oneline --all"
# Livereload helpers
## add current dir to livereload watchlist
alias lron='open "livereload:add?path=`pwd`"'
## remove current dir from livereload watchlist
alias lroff='open "livereload:remove?path=`pwd`"'
